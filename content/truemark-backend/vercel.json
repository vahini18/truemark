{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "maxLambdaSize": "50mb"
    }
  },
  "ignore": [
    "content/truemark-backend/large_unused_files/*"
  ]
}
